key: 'superscript'

patterns: [
  # Matches superscript
  #
  # Examples
  #
  #   ^superscript^
  #
  name: 'markup.superscript.asciidoc'
  match: '(?<!\\\\)(\\[.+?\\])?((\\^)(\\S+?)(\\^))'
  captures:
    1: name: 'markup.meta.attribute-list.asciidoc'
    2: name: 'markup.super.superscript.asciidoc'
    3: name: 'punctuation.definition.super.asciidoc'
    5: name: 'punctuation.definition.super.asciidoc'
]
