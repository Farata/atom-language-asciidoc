key: 'table-csv'

patterns: [

  name: 'markup.table.csv.asciidoc'
  begin: '^(,===)$'
  beginCaptures:
    0: name: 'constant.other.symbol.asciidoc'
  contentName: 'string.unquoted.asciidoc'
  patterns: [
    comment: 'cell separator'
    match: ','
    captures:
      0: name: 'constant.other.symbol.asciidoc'
  ,
    include: '#general-block-macro'
  ]
  end: '^(\\1)$'
  endCaptures:
    0: name: 'constant.other.symbol.asciidoc'
]
